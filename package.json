    },
    "./dist/style.css": "./dist/style.css"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build && vue-tsc --build --emitDeclarationOnly --declaration",
    "preview": "vite preview"
  },
  "dependencies": {
    "path": "^0.12.7",
    "vue": "^3.3.4"
  },
  "peerDependencies": {
    "pinia": "^2.1.3",
    "splitpanes": "^3.1.5",
    "vue": "^3.3.4",
    "vuedraggable": "^4.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.2.1",
    "@types/splitpanes": "^2.2.1",
    "@vitejs/plugin-vue": "^4.1.0",
    "sass": "^1.62.1",
    "typescript": "^5.0.2",
    "vite": "^4.3.2",
    "vue-tsc": "^1.4.2"
  }
